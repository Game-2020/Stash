#!desc=京东比价 (内置版)
name: jdprice
desc: 京东比价
http:
  mitm:
    - "api.m.jd.com"

  script:
    - match: ^https?://api\.m\.jd\.com/client\.action\?functionId=(wareBusiness|serverConfig|basicConfig)
      name: jdprice
      type: response
      require-body: true
      timeout: 10

